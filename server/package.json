  },
  "dependencies": {
    "@keystone-6/auth": "^6.0.0",
    "@keystone-6/core": "^4.0.1",
    "@keystone-6/fields-document": "^6.0.0",
    "got": "^12.5.3",
    "sharp": "^0.31.2",
    "typescript": "^4.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bytes": "^3.1.1",
    "@types/sharp": "^0.31.0",
    "tsx": "^3.12.1"
  }
}
